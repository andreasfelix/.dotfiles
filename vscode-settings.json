{
  "editor.rulers": [
    88,
    100,
    120
  ],
  "[python]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 4
  },
  "[gdscript]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 4,
  },
  "vim.leader": ",",
  "vim.useCtrlKeys": false,
  "vim.handleKeys": {
    "<C-c>": false,
    "<C-d>": false,
    "<A-S-Up>": false,
    "<A-S-Down>": false
  },
  "vim.easymotion": true,
  "vim.camelCaseMotion.enable": true,
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": [
        " "
      ],
      "after": [
        "<leader>",
        "<leader>",
        "<leader>",
        "b",
        "d",
        "w"
      ]
    },
    {
      "before": [
        ">"
      ],
      "after": [
        ">",
        ">"
      ]
    },
    {
      "before": [
        "<"
      ],
      "after": [
        "<",
        "<"
      ]
    }
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": [
        ">"
      ],
      "commands": [
        "editor.action.indentLines"
      ]
    },
    {
      "before": [
        "<"
      ],
      "commands": [
        "editor.action.outdentLines"
      ]
    }
  ],
  "vim.easymotionMarkerFontSize": "22",
  "vim.easymotionMarkerBackgroundColor": "#434CFF",
  "vim.easymotionMarkerFontFamily": "Inconsolata",
  "vim.easymotionMarkerHeight": 22,
  "vim.easymotionMarkerWidthPerChar": 12,
  "emmet.triggerExpansionOnTab": true,
  "window.zoomLevel": 0,
  "files.autoSave": "off",
  "editor.tabSize": 2,
  "liveshare.featureSet": "insiders",
  "bracket-pair-colorizer-2.colors": [
    "#0a3d62",
    "#e58e26",
    "#079992"
  ],
  "bracket-pair-colorizer-2.unmatchedScopeColor": "#b71540",
  "bracket-pair-colorizer-2.showHorizontalScopeLine": false,
  "workbench.colorCustomizations": {
    "editorCursor.foreground": "#B3ECFF",
    "editorCursor.background": "#000000",
    "editor.selectionBackground": "#B3ECFF77",
    "editor.inactiveSelectionBackground": "#B3ECFF77",
    "[Solarized Custom Light]": {
      "sideBar.background": "#F5F5F5",
      "activityBar.background": "#f5f5f5",
      "statusBar.background": "#f5f5f5",
      "tab.activeBackground": "#f5f5f5",
      "tab.inactiveBackground": "#e4e4e4",
      "editor.background": "#F5F5F5",
      "editor.foreground": "#23476a"
      // "editor.background": "#FFFAEA"
    }
  },
  "editor.renderLineHighlight": "gutter",
  "editor.cursorBlinking": "solid",
  "bracket-pair-colorizer-2.showVerticalScopeLine": false,
  "editor.matchBrackets": false,
  "editor.fontSize": 14,
  "editor.fontFamily": "Ubuntu Mono",
  "workbench.editor.tabSizing": "shrink",
  "latex-workshop.view.pdf.viewer": "external",
  "[latex]": {
    "editor.defaultFormatter": "James-Yu.latex-workshop"
  },
  "workbench.colorTheme": "Solarized Custom Light",
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.formatOnSave": true,
  "window.menuBarVisibility": "visible",
  "window.titleBarStyle": "custom",
  "terminal.integrated.inheritEnv": false,
  "git.autofetch": true,
  "git.confirmSync": false,
  "explorer.confirmDragAndDrop": false,
  "diffEditor.ignoreTrimWhitespace": false,
  "python.formatting.provider": "black",
  "python.linting.mypyEnabled": true,
  "editor.tabCompletion": "on",
  "autoDocstring.docstringFormat": "sphinx",
  "liveServer.settings.donotShowInfoMsg": true,
  "gitlens.advanced.telemetry.enabled": false,
  "editor.wordWrap": "on",
  "editor.minimap.enabled": false,
  "editor.scrollBeyondLastColumn": 0,
  "C_Cpp.updateChannel": "Insiders",
  "python.dataScience.askForKernelRestart": false,
}